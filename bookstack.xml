<?xml version="1.0"?>
<Container version="2">
  <Name>bookstack</Name>
  <Repository>solidnerd/bookstack</Repository>
  <Registry>https://hub.docker.com/r/solidnerd/bookstack/~/dockerfile/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/solidnerd/bookstack/</Support>
  <Project/>
  <Overview>BookStack is a simple, self-hosted, easy-to-use platform for organising and storing information.</Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:80]</WebUI>
  <TemplateURL/>
  <Icon>https://avatars3.githubusercontent.com/u/20912696?s=200&amp;amp;v=4</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DonateText/>
  <DonateLink/>
  <Description>BookStack is a simple, self-hosted, easy-to-use platform for organising and storing information.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort></HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/var/www/bookstack/public/uploads</ContainerDir>
      <Mode>rw,slave</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/var/www/bookstack/public/storage</ContainerDir>
      <Mode>rw,slave</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value></Value>
      <Name>DB_HOST</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>DB_USERNAME</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>DB_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>DB_DATABASE</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Host Key 1" Target="DB_HOST" Default="" Mode="" Description="Container Variable: DB_HOST" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="Host Key 2" Target="DB_USERNAME" Default="" Mode="" Description="Container Variable: DB_USERNAME" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="Host Key 3" Target="DB_PASSWORD" Default="" Mode="" Description="Container Variable: DB_PASSWORD" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="Host Key 4" Target="DB_DATABASE" Default="" Mode="" Description="Container Variable: DB_DATABASE" Type="Variable" Display="always" Required="false" Mask="false"></Config>
  <Config Name="Host Port 1" Target="80" Default="" Mode="tcp" Description="Container Port: 80" Type="Port" Display="always" Required="false" Mask="false"></Config>
  <Config Name="Host Path 1" Target="/var/www/bookstack/public/uploads" Default="" Mode="rw,slave" Description="Container Path: /var/www/bookstack/public/uploads" Type="Path" Display="always" Required="false" Mask="false"></Config>
  <Config Name="Host Path 2" Target="/var/www/bookstack/public/storage" Default="" Mode="rw,slave" Description="Container Path: /var/www/bookstack/public/storage" Type="Path" Display="always" Required="false" Mask="false"></Config>
</Container>
