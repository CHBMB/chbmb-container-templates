<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>cloudflare</Name>
  <Description>Cloudflare dynamic DNS updater.  Taken from the container by tsaikd and xml modified for Unraid.</Description>
  <Registry>https://hub.docker.com/r/tsaikd/cloudflare/</Registry>
  <Repository>tsaikd/cloudflare</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>CFKEY</Name>
      <Value>CLOUDFLARE-APIKEY</Value>
    </Variable>
    <Variable>
      <Name>CFUSER</Name>
      <Value>EMAIL-USED-TO-REGISTER</Value>
    </Variable>
    <Variable>
      <Name>CFHOST</Name>
      <Value>WWW.SERVER.com,SUBDOMAIN1.SERVER.COM,SUBDOMAIN2.SERVER.COM</Value>
    </Variable>
    <Variable>
      <Name>CFZONE</Name>
      <Value>SERVER.COM</Value>
    </Variable>
    <Variable>
      <Name>CFINTERVAL</Name>
      <Value>300</Value>
    </Variable>
    <Variable>
      <Name>CFONCE</Name>
      <Value>false</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish/>
  </Networking>
  <Data/>
  <Version></Version>
  <WebUI></WebUI>
  <Banner></Banner>
  <Icon>http://i.imgur.com/PKa3Sm2.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
