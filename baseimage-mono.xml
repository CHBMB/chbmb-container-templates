<?xml version="1.0"?>
<Container version="2">
  <Name>baseimage-mono</Name>
  <Repository>lsiobase/mono</Repository>
  <Registry/>
  <Network>docker</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview>LinuxServer.io Mono base image built with S6 overlay.[br][br]&#xD;
&#xD;
[b][span style='color: #E80000;']After first pull exec into container and run:[/span][/b][br]&#xD;
rm -rf /config/etc/cont-init.d/30-config &amp;amp;&amp;amp; \[br]&#xD;
rm -rf /config/etc/services.d/app/run &amp;amp;&amp;amp; \[br]&#xD;
echo -e '#!/usr/bin/with-contenv bash' &amp;amp;gt; /config/etc/services.d/app/run &amp;amp;&amp;amp; \[br]&#xD;
echo -e '#!/usr/bin/with-contenv bash' &amp;amp;gt; /config/etc/cont-init.d/30-config[br]&#xD;</Overview>
  <Category>Productivity: Tools: Status:Beta</Category>
  <WebUI>http://[IP]:[PORT:]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/linuxserver-ls-logo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Description>LinuxServer.io Mono base image built with S6 overlay.[br][br]&#xD;
&#xD;
[b][span style='color: #E80000;']After first pull exec into container and run:[/span][/b][br]&#xD;
rm -rf /config/etc/cont-init.d/30-config &amp;amp;&amp;amp; \[br]&#xD;
rm -rf /config/etc/services.d/app/run &amp;amp;&amp;amp; \[br]&#xD;
echo -e '#!/usr/bin/with-contenv bash' &amp;amp;gt; /config/etc/services.d/app/run &amp;amp;&amp;amp; \[br]&#xD;
echo -e '#!/usr/bin/with-contenv bash' &amp;amp;gt; /config/etc/cont-init.d/30-config[br]&#xD;</Description>
  <Networking>
    <Mode>docker</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/disks/virtualisation/appdata/baseimage-mono</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/disks/virtualisation/appdata/baseimage-mono/etc/cont-init.d/30-config</HostDir>
      <ContainerDir>/etc/cont-init.d/30-config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/disks/virtualisation/appdata/baseimage-mono/etc/services.d/app/run</HostDir>
      <ContainerDir>/etc/services.d/app/run</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="PUID" Target="PUID" Default="" Mode="" Description="PUID" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
  <Config Name="PGID" Target="PGID" Default="" Mode="" Description="PGID" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
  <Config Name="appdata" Target="/config" Default="/mnt/disks/virtualisation/appdata/baseimage-mono" Mode="rw" Description="Container Path: /config" Type="Path" Display="always" Required="false" Mask="false">/mnt/disks/virtualisation/appdata/baseimage-mono</Config>
  <Config Name="Config" Target="/etc/cont-init.d/30-config" Default="" Mode="rw" Description="Container Path: /etc/cont-init.d/30-config" Type="Path" Display="always" Required="false" Mask="false">/mnt/disks/virtualisation/appdata/baseimage-mono/etc/cont-init.d/30-config</Config>
  <Config Name="Init" Target="/etc/services.d/app/run" Default="" Mode="rw" Description="Container Path: /etc/services.d/app/run" Type="Path" Display="always" Required="false" Mask="false">/mnt/disks/virtualisation/appdata/baseimage-mono/etc/services.d/app/run</Config>
</Container>
